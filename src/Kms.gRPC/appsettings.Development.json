{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Grpc": "Information",
      "Microsoft": "Information"
    }
  },
  "Redis": {
    "Host": "localhost:6379",
    "Timeout": 300000,
    "ConnectRetry": 2
  },
  "Kms": {
    "KeyVault": {
      "Symmetric": {
        "DefaultKeyExpire": {
          "Second": 60
        }
      },
      "SharedSecret": {
        "DefaultKeyExpire": {
          "Second": 60
        }
      },
      "Asymmetric": {
        "DefaultKeyExpire": {
          "Second": 60
        }
      }
    }
  }
}
